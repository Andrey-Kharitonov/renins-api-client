{
    "name": "renins/api-client-link-auto",
    "description": "PHP client for Renaissance Auto Integration API",
    "type": "library",
    "keywords": ["API", "Renins", "SOAP", "Rest"],
    "homepage": "http://www.renins.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "support",
            "email": "support@renins.com"
        }
