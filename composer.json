        "ext-soap": "*",
        "ext-curl": "*",
        "ext-xml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "monolog/monolog": "^1.23"
    },
    "support": {
        "email": "support@renins.com"
    },
    "autoload": {
        "psr-0": { "ReninsApi\\": "lib/" }
    },
    "extra": {
    },
    "config": {
        "bin-dir": "bin",
        "process-timeout": 600
    }
}
